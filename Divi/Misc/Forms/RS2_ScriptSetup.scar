// Main Form - OnClose
procedure _DontUse_OSI_Form_RS2_ScriptSetup_OnClose(Sender: TObject; var Action: TCloseAction);
var
  Path: string;
begin          
  if (Sender = OSI_Form_RS2_ScriptSetup.OSI_Form_RS2_ScriptSetup) then
  begin                                                                                      
    Path := ScriptPath + 'RS2_ScriptSetup_Settings.ini';                              
    WriteINI('_DontUse_OSI_Form_RS2_ScriptSetup', 'OverrideOptions', BoolToStr(OSI_Form_RS2_ScriptSetup.OverrideOptions), Path);
    WriteINI('_DontUse_OSI_Form_RS2_ScriptSetup', 'WAVPath', OSI_Form_RS2_ScriptSetup_Settings.WAVPath, Path);                          
    WriteINI('_DontUse_OSI_Form_RS2_ScriptSetup', 'SplashPath', OSI_Form_RS2_ScriptSetup_Settings.SplashPath, Path);                         
    WriteINI('_DontUse_OSI_Form_RS2_ScriptSetup', 'BackColorBool', BoolToStr(OSI_Form_RS2_ScriptSetup_Settings.DefaultFormBackColorBool), Path);
    WriteINI('_DontUse_OSI_Form_RS2_ScriptSetup', 'BackColor', IntToStr(OSI_Form_RS2_ScriptSetup_Settings.DefaultFormBackColor), Path);
    Action := caFree;                  
  end;          
end;

// Main Form - Load Default INI
procedure _DontUse_OSI_Form_RS2_ScriptSetup_LoadDefaultIni;
var
  s, Path: string; 
  Bmp: TSCARBitmap;
  B: Boolean;
begin            
   try  
      Path := ScriptPath + 'RS2_ScriptSetup_Settings.ini';                          
      s := ReadINI('_DontUse_OSI_Form_RS2_ScriptSetup', 'OverrideOptions', Path);
      if StrIsBool(s) then 
      OSI_Form_RS2_ScriptSetup.OverrideOptions := StrToBool(s);
      if (OSI_Form_RS2_ScriptSetup.OverrideOptions) or (not OSI_Form_RS2_ScriptSetup.LoadedSettingsFromFunct) then 
      begin
        OSI_Form_RS2_ScriptSetup_Settings.WAVPath := ReadINI('_DontUse_OSI_Form_RS2_ScriptSetup', 'WAVPath', Path);                          
        OSI_Form_RS2_ScriptSetup_Settings.SplashPath := ReadINI('_DontUse_OSI_Form_RS2_ScriptSetup', 'SplashPath', Path);
        s := ReadINI('_DontUse_OSI_Form_RS2_ScriptSetup', 'BackColorBool', Path);
        if StrIsBool(s) then
          OSI_Form_RS2_ScriptSetup_Settings.DefaultFormBackColorBool := StrToBool(s);
        s := ReadINI('_DontUse_OSI_Form_RS2_ScriptSetup', 'BackColor', Path);
        if StrIsInt(s) then        
          OSI_Form_RS2_ScriptSetup_Settings.DefaultFormBackColor := StrToInt(s);
      end;   
    finally    
    end;                                                                                                                                    
    try                                                 
      s := OSI_Form_RS2_ScriptSetup_Settings.WAVPath; 
      if EndsWith('.wav', lowerCase(s)) then
        PlaySound(OSI_Form_RS2_ScriptSetup_Settings.WAVPath);
      s := OSI_Form_RS2_ScriptSetup_Settings.SplashPath; 
      if EndsWith('.png', LowerCase(s)) or EndsWith('.bmp', LowerCase(s)) or EndsWith('.jpeg', LowerCase(s)) then
      begin
        Bmp := TSCARBitmap.Create('');
        if EndsWith('.png', LowerCase(s)) then
          B := Bmp.LoadFromPng(s);
        if EndsWith('.bmp', Lowercase(s)) then
          B := Bmp.LoadFromBmp(s);
        if EndsWith('.jpeg', LowerCase(s)) then
          B := Bmp.LoadFromJpeg(s);
        if B then
        begin
          DebugBitmap(Bmp);
          wait(4000);
          HideDebugImgWindow;
        end;    
        Bmp.Free;
      end;      
    finally               
    end;          
end;

