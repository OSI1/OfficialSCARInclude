// Options Form - Close
procedure _DontUse_OSI_Form_RS2_ScriptSetup_OptionsClose(Sender: TObject; var Action: TCloseAction);
begin
  if (Sender = _DontUse_OSI_Form_RS2_ScriptSetup_Options._DontUse_OSI_Form_RS2_ScriptSetup_Options) then
  begin
    OSI_Form_RS2_ScriptSetup.OSI_Form_RS2_ScriptSetup.Enabled := True;
    Action := caFree;    
  end;
end;

// Options Form - Wav Browse Click
procedure _DontUse_OSI_Form_RS2_ScriptSetup_btnWavBrowseClick(Sender: TObject);
begin
  if (Sender = _DontUse_OSI_Form_RS2_ScriptSetup_Options.btnWavBrowse) then
  begin           
    _DontUse_OSI_Form_RS2_ScriptSetup_Options.openWavDialog := TOpenDialog.Create(_DontUse_OSI_Form_RS2_ScriptSetup_Options._DontUse_OSI_Form_RS2_ScriptSetup_Options);     
    with _DontUse_OSI_Form_RS2_ScriptSetup_Options.openWavDialog do
    begin     
      InitialDir := ScriptPath;
      Filter := 'Waveform Audio File|*.wav';
      FilterIndex := 1;
      Title := 'Select WAV file';             
      Options := [ofFileMustExist, ofPathMustExist];
    end;
    if _DontUse_OSI_Form_RS2_ScriptSetup_Options.openWavDialog.Execute then  
      if EndsWith('.wav', Lowercase(_DontUse_OSI_Form_RS2_ScriptSetup_Options.openWavDialog.FileName)) then 
      begin  
        _DontUse_OSI_Form_RS2_ScriptSetup_Options.editWAVPath.Text := _DontUse_OSI_Form_RS2_ScriptSetup_Options.openWavDialog.FileName;
        OSI_Form_RS2_ScriptSetup_Settings.WAVPath := _DontUse_OSI_Form_RS2_ScriptSetup_Options.editWAVPath.Text;        
      end
      else
        TaskMessageDlg('Wrong file type: ', 'Must be a .wav file!', mtWarning, [mbClose]);
    _DontUse_OSI_Form_RS2_ScriptSetup_Options.openWavDialog.Free;    
  end;  
end;

// Options Form - Spash Image Browse Click
procedure _DontUse_OSI_Form_RS2_ScriptSetup_Options_SplashClick(Sender: TObject);
var
  s: string;
begin
  if (Sender = _DontUse_OSI_Form_RS2_ScriptSetup_Options.btnSplashBrowse) then
  begin        
    _DontUse_OSI_Form_RS2_ScriptSetup_Options.openSplashDialog := TOpenDialog.Create(_DontUse_OSI_Form_RS2_ScriptSetup_Options._DontUse_OSI_Form_RS2_ScriptSetup_Options);
    with _DontUse_OSI_Form_RS2_ScriptSetup_Options.openSplashDialog do
    begin
      InitialDir := ScriptPath;
      Filter := 'Image File|*.png;*.jpeg;*.bmp|';
      FilterIndex := 1;
      Title := 'Select Image file';
      Options := [ofFileMustExist, ofPathMustExist];
    end;    
    if _DontUse_OSI_Form_RS2_ScriptSetup_Options.openSplashDialog.Execute then
    begin
      s := Lowercase(_DontUse_OSI_Form_RS2_ScriptSetup_Options.openSplashDialog.FileName);   
      if EndsWith('.png', s) or EndsWith('.jpeg', s) or EndsWith('.bmp', s) then
      begin    
        _DontUse_OSI_Form_RS2_ScriptSetup_Options.editSplashPath.Text := _DontUse_OSI_Form_RS2_ScriptSetup_Options.openSplashDialog.FileName;
        OSI_Form_RS2_ScriptSetup_Settings.SplashPath := _DontUse_OSI_Form_RS2_ScriptSetup_Options.editSplashPath.Text;  
      end
      else
        TaskMessageDlg('Wrong file type: ', 'Must be one of the supported image file types!', mtWarning, [mbClose]);     
    end;
    _DontUse_OSI_Form_RS2_ScriptSetup_Options.openSplashDialog.Free;    
  end;  
end;

// For Saving Settings 
// Does all the basic operations for Save Dialog, returns if it executed or not, then returns the save path
function _DontUse_OSI_Form_RS2_ScriptSetup_SaveDOpen(Title: string; var Path: string; Form: TForm): Boolean;
begin
  _DontUse_OSI_Form_RS2_ScriptSetup_SaveDialog := TSaveDialog.Create(Form);
  with _DontUse_OSI_Form_RS2_ScriptSetup_SaveDialog do
  begin
    InitialDir := ScriptPath;
    Filter := 'Settings File|*.ini|';
    FilterIndex := 1;
    Title := Title;
    Options := [ofPathMustExist]; 
  end;               
  Result := _DontUse_OSI_Form_RS2_ScriptSetup_SaveDialog.Execute;
  Path := _DontUse_OSI_Form_RS2_ScriptSetup_SaveDialog.FileName;
  _DontUse_OSI_Form_RS2_ScriptSetup_SaveDialog.Free;     
end;

// For Opening Settings
// Does all the basic operations for Open Dialog, returns if it executed or not, then returns the save path
function _DontUse_OSI_Form_RS2_ScriptSetup_OpenD(Title: string; var Path: string; Form: TForm): Boolean;
begin
  _DontUse_OSI_Form_RS2_ScriptSetup_OpenDialog := TOpenDialog.Create(Form);
  with _DontUse_OSI_Form_RS2_ScriptSetup_OpenDialog do
  begin
    InitialDir := ScriptPath;
    Filter := 'Settings File|*.ini|';
    FilterIndex := 1;
    Title := Title;
    Options := [ofPathMustExist, ofFileMustExist]; 
  end;               
  Result := _DontUse_OSI_Form_RS2_ScriptSetup_OpenDialog.Execute;
  Path := _DontUse_OSI_Form_RS2_ScriptSetup_OpenDialog.FileName;
  _DontUse_OSI_Form_RS2_ScriptSetup_OpenDialog.Free;     
end;

// Options Form - Save Options Click
procedure _DontUse_OSI_Form_RS2_ScriptSetup_btnSaveClick(Sender: TObject);
var
  Path: string;
begin
  if (Sender = _DontUse_OSI_Form_RS2_ScriptSetup_Options.btnSaveOpt) then
    if _DontUse_OSI_Form_RS2_ScriptSetup_SaveDOpen('Save Options', Path, _DontUse_OSI_Form_RS2_ScriptSetup_Options._DontUse_OSI_Form_RS2_ScriptSetup_Options) then
    begin                                                                                                     
      if EndsWith('.ini', Path) then                                                                          
        begin                                                                                                 
          try                                                             
            WriteINI('_DontUse_OSI_Form_RS2_ScriptSetup', 'WAVPath', OSI_Form_RS2_ScriptSetup_Settings.WAVPath, Path)                          
            WriteINI('_DontUse_OSI_Form_RS2_ScriptSetup', 'SplashPath', OSI_Form_RS2_ScriptSetup_Settings.SplashPath, Path);
            WriteINI('_DontUse_OSI_Form_RS2_ScriptSetup', 'BackColorBool', BoolToStr(OSI_Form_RS2_ScriptSetup_Settings.DefaultFormBackColorBool), Path);
            WriteINI('_DontUse_OSI_Form_RS2_ScriptSetup', 'BackColor', IntToStr(OSI_Form_RS2_ScriptSetup_Settings.DefaultFormBackColor), Path);
            MessageDlg('Successfully Saved!', mtInformation, [mbClose]);                        
          except
            TaskMessageDlg('Invalid Path: ', 'Failed to save!', mtWarning, [mbClose]);                      
          end;
        end      
      else
        TaskMessageDlg('Wrong file type: ', 'Must save as .ini file!', mtWarning, [mbClose]);            
    end;  
end;

// Options Form - Load Options Click
procedure _DontUse_OSI_Form_RS2_ScriptSetup_btnLoadClick(Sender: TObject);
var
  Path: string;
begin        
  if (Sender = _DontUse_OSI_Form_RS2_ScriptSetup_Options.btnLoadOpt) then
    if _DontUse_OSI_Form_RS2_ScriptSetup_OpenD('Load Options', Path, _DontUse_OSI_Form_RS2_ScriptSetup_Options._DontUse_OSI_Form_RS2_ScriptSetup_Options) then
      begin
        if EndsWith('.ini', Path) then 
          begin                                                                                                             
            OSI_Form_RS2_ScriptSetup_Settings.WAVPath := ReadINI('_DontUse_OSI_Form_RS2_ScriptSetup', 'WAVPath', Path);                          
            OSI_Form_RS2_ScriptSetup_Settings.SplashPath := ReadINI('_DontUse_OSI_Form_RS2_ScriptSetup', 'SplashPath', Path);
            OSI_Form_RS2_ScriptSetup_Settings.DefaultFormBackColorBool := StrToBool(ReadINI('_DontUse_OSI_Form_RS2_ScriptSetup', 'BackColorBool', Path));
            OSI_Form_RS2_ScriptSetup_Settings.DefaultFormBackColor := StrToInt(ReadINI('_DontUse_OSI_Form_RS2_ScriptSetup', 'BackColor', Path));
            if OSI_Form_RS2_ScriptSetup_Settings.DefaultFormBackColorBool then            
              OSI_Form_RS2_ScriptSetup.OSI_Form_RS2_ScriptSetup.Color := OSI_Form_RS2_ScriptSetup_Settings.DefaultFormBackColor; 
            _DontUse_OSI_Form_RS2_ScriptSetup_Options.editSplashPath.Text := OSI_Form_RS2_ScriptSetup_Settings.SplashPath;
            _DontUse_OSI_Form_RS2_ScriptSetup_Options.editWAVPath.Text := OSI_Form_RS2_ScriptSetup_Settings.WAVPath;            
            MessageDlg('Successfully Loaded!', mtInformation, [mbClose]);            
          end      
        else
          TaskMessageDlg('Wrong file type: ', 'Must be .ini file!', mtWarning, [mbClose]);            
      end; 
end;

