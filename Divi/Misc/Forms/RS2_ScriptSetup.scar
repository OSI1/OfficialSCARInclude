        edtScriptVersion.Text := OSI_ScriptInfo.Version;
        memoScriptDesc.Text := OSI_ScriptInfo.Description;
        edtScriptLastUpdated.Text := OSI_ScriptInfo.LastUpdated;
        edtScriptCreated.Text := OSI_ScriptInfo.Created;        
      end;                   
